{
  "name": "trilha-da-fe",
    "type": "module",
    "version": "1.0.0",
  "description": "Trilha da Fé — aplicativo PWA de estudo bíblico, quizzes e desafios espirituais.",
    "private": true,
    "scripts": {
      "dev": "vite",
  "build": "node scripts/insert-sha.js && node scripts/generate-icons.js && vite build",
  "insert-sha": "node scripts/insert-sha.js",
      "preview": "vite preview",
      "pwa:icons": "node scripts/generate-icons.js",
      "pwa:test": "npm run build && npx serve dist",
      "pwa:lighthouse": "npx lighthouse http://localhost:3000 --view --output=html --output-path=./lighthouse-report.html"
    },
    "dependencies": {
      "@radix-ui/react-alert-dialog": "^1.0.5",
      "@radix-ui/react-avatar": "^1.0.3",
      "@radix-ui/react-checkbox": "^1.0.4",
      "@radix-ui/react-dialog": "^1.0.5",
      "@radix-ui/react-dropdown-menu": "^2.0.5",
      "@radix-ui/react-label": "^2.0.2",
      "@radix-ui/react-slot": "^1.0.2",
      "@radix-ui/react-tabs": "^1.0.4",
      "@radix-ui/react-toast": "^1.1.5",
      "@radix-ui/react-slider": "^1.1.2",
      "@react-three/drei": "^9.88.7",
      "@react-three/fiber": "^8.15.8",
      "class-variance-authority": "^0.7.0",
      "clsx": "^2.0.0",
      "framer-motion": "^10.16.4",
      "lucide-react": "^0.285.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-helmet": "^6.1.0",
      "tailwind-merge": "^1.1.4",
      "tailwindcss-animate": "^1.0.7",
      "three": "^0.157.0"
