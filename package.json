    "devDependencies": {
      "@babel/generator": "^7.27.0",
      "@babel/parser": "^7.27.0",
      "@babel/traverse": "^7.27.0",
      "@babel/types": "^7.27.0",
      "@types/node": "^20.8.3",
      "@types/react": "^18.2.15",
      "@types/react-dom": "^18.2.7",
      "@vitejs/plugin-react": "^4.0.3",
      "autoprefixer": "^10.4.16",
      "postcss": "^8.4.31",
      "tailwindcss": "^3.3.3",
      "vite": "^4.4.5",
      "terser": "^5.39.0",
      "eslint": "^8.57.1",
      "eslint-config-react-app": "^7.0.1",
      "vite-plugin-svgr": "^4.2.0"
